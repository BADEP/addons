<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    <record id="project_offer_public" model="ir.rule">
        <field name="name">Offer Positions: Public</field>
        <field name="model_id" ref="project_submission.model_project_offer"/>
        <field name="domain_force">[('website_published', '=', True)]</field>
        <field name="groups" eval="[(4, ref('base.group_public'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>
    <record id="project_offer_portal" model="ir.rule">
        <field name="name">offer Positions: Portal</field>
        <field name="model_id" ref="project_submission.model_project_offer"/>
        <field name="domain_force">[('website_published', '=', True)]</field>
        <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>
    <record id="project_offer_officer" model="ir.rule">
        <field name="name">Offer Positions: Poject Officer</field>
        <field name="model_id" ref="project_submission.model_project_offer"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>
    <record id="project_offer_type_public" model="ir.rule">
        <field name="name">Offer Type: Public</field>
        <field name="model_id" ref="project_submission.model_project_offer_type"/>
        <field name="domain_force">[('offer.website_published', '=', True)]</field>
        <field name="groups" eval="[(4, ref('base.group_public'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>
</data>
</openerp>